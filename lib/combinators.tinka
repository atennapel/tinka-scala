let id <l> {A : Type l} (x : A) : A = x;
let the <l> (A : Type l) (x : A) : A = x;
let const <l k> {A : Type l} {B : Type k} (x : A) (y : B) : A = x;
let flip <l k m> {A : Type l} {B : Type k} {C : Type m} (f : A -> B -> C) (x : B) (y : A) : C = f y x;
let compose <l k m> {A : Type l} {B : Type k} {C : Type m} (f : B -> C) (g : A -> B) (x: A) : C = f (g x);
let (<<) = compose;

